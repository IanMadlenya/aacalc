<VirtualHost *:443>
        ServerName aacalc.com
	Redirect permanent / https://www.aacalc.com/

        SSLEngine on
        SSLCertificateFile      /home/ubuntu/oaa.data/aacalc.com.crt
        SSLCertificateKeyFile   /home/ubuntu/oaa.data/www.aacalc.com.key
        SSLCertificateChainFile /home/ubuntu/oaa.data/gd_bundle.crt
</VirtualHost>
